{
  "expo": {
    "name": "WhatTheWeather",
    "description": "Super Cool Weather Application!",
    "slug": "WhatTheWeather",
    "ios": {
      "bundleIdentifier": "com.heebeancreative.WhatTheWeather",
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "The location data will be used for getting weather data ONLY. We DO NOT store, process, or share your private data."
      }
    },
    "android": {
      "package": "com.heebeancreative.WhatTheWeather",
      "versionCode": 16,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.20",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",

    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"]
  }
}
